<%= bootstrap_form_tag(url: add_crash_main_page_index_url, method: :post) do |f| %>
  <%= f.text_field :vin, value: @vin %>
  <%= f.text_field :vinOthers %>
  <%= f.text_area :damages %>
  <%= content_tag :div, class: 'crash-datepicker-div-JS' do %>
    <%= f.text_field :crashdate, class: 'crash-datepicker-JS', placeholder: ('crashdate'), label: ('crashdate') %>
  <% end %>

  <%= f.submit 'Add' %>
<% end %>